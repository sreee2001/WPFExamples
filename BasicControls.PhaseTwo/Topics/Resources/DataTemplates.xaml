<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Button_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.Button"
    xmlns:TextBox_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.TextBox"
    xmlns:CheckBox_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.CheckBox"
    xmlns:RadioButton_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.RadioButton"
    xmlns:PasswordBox_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.PasswordBox"
    xmlns:Image_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.Image"
    xmlns:common="clr-namespace:BasicControls.Common;assembly=BasicControls"
    xmlns:sampleViews="clr-namespace:BasicControls.PhaseTwo.Topics.SampleViews"
    >
    <DataTemplate DataType="{x:Type common:ControlIntroductionViewModel}">
        <common:ControlIntroductionView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type Button_ns:ButtonSampleViewModel}">
        <sampleViews:ButtonSampleView />
    </DataTemplate>


    <DataTemplate DataType="{x:Type TextBox_ns:TextBoxSampleViewModel}">
        <sampleViews:TextBoxSampleView />
    </DataTemplate>


    <DataTemplate DataType="{x:Type CheckBox_ns:CheckBoxSampleViewModel}">
        <sampleViews:CheckBoxSampleView />
    </DataTemplate>


    <DataTemplate DataType="{x:Type RadioButton_ns:RadioButtonSampleViewModel}">
        <sampleViews:RadioButtonSampleView />
    </DataTemplate>


    <DataTemplate DataType="{x:Type PasswordBox_ns:PasswordBoxSampleViewModel}">
        <sampleViews:PasswordBoxSampleView />
    </DataTemplate>


    <DataTemplate DataType="{x:Type Image_ns:ImageSampleViewModel}">
        <sampleViews:ImageSampleView />
    </DataTemplate>

</ResourceDictionary>