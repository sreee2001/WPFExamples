<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:Button_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.Button"
   xmlns:TextBox_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.TextBox"
   xmlns:CheckBox_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.CheckBox"
   xmlns:RadioButton_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.RadioButton"
   xmlns:PasswordBox_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.PasswordBox"
   xmlns:Image_ns="clr-namespace:BasicControls.PhaseTwo.Topics.AutoGeneratedTopics.Image"
   >


   <DataTemplate DataType="{x:Type Button_ns:ButtonIntroductionViewModel}">
        <Button_ns:ButtonIntroductionView />
    </DataTemplate>


   <DataTemplate DataType="{x:Type TextBox_ns:TextBoxIntroductionViewModel}">
        <TextBox_ns:TextBoxIntroductionView />
    </DataTemplate>


   <DataTemplate DataType="{x:Type CheckBox_ns:CheckBoxIntroductionViewModel}">
        <CheckBox_ns:CheckBoxIntroductionView />
    </DataTemplate>


   <DataTemplate DataType="{x:Type RadioButton_ns:RadioButtonIntroductionViewModel}">
        <RadioButton_ns:RadioButtonIntroductionView />
    </DataTemplate>


   <DataTemplate DataType="{x:Type PasswordBox_ns:PasswordBoxIntroductionViewModel}">
        <PasswordBox_ns:PasswordBoxIntroductionView />
    </DataTemplate>


   <DataTemplate DataType="{x:Type Image_ns:ImageIntroductionViewModel}">
        <Image_ns:ImageIntroductionView />
    </DataTemplate>

</ResourceDictionary>